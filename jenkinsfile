   agent any 
    stages {
        stage('Git SCM') {
            steps {
                checkout([$class: 'GitSCM', branches: [[name: '*/master']], doGenerateSubmoduleConfigurations: false, extensions: [], submoduleCfg: [], userRemoteConfigs: [[credentialsId: 'SudirCreds', url: 'https://github.com/Songhai-Tech/SCSS.git']]])
 
                   }      
         }
     }
    }
