pipeline {
   agent any 
    stages {
        stage('Stage 1') {
            steps {
                echo 'Im Sulayman' 
        }
        }
        stage('Stage 2') {
        	steps {
        		echo 'This is an Edit made by Woody'
        	}
        }
        stage('Stage 3') {
        	steps {
        		echo 'This is the 3rd stage of my first pipeline'
        	}
        }
    }
}
